<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center">

    <fragment
        android:id="@+id/gestureMapFragment"
        android:name="zkrtdrone.zkrt.com.maplib.info.GestureMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_gesture_map"/>

    <ImageView
        android:id="@+id/img_common_exchange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/gs_map_widget_hide_in"/>

    <ImageView
        android:id="@+id/img_common_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|top"
        android:background="@mipmap/gs_map_widget_hide_in_litchi"/>

    <ImageView
        android:id="@+id/img_common_clear_pryline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|bottom"
        android:background="@mipmap/gs_map_widget_earaser_big"/>
</merge>